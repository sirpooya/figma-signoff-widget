{
  "name": "Design-SignOff",
  "version": "1.0.0",
  "description": "Figma widget for design sign-off process",
  "scripts": {
    "test": "tsc -p src --noEmit",
    "build": "esbuild src/code.tsx --bundle --outfile=dist/code.js --format=esm --target=es6 --jsx=transform --jsx-factory=figma.widget.h --jsx-fragment=figma.widget.Fragment",
    "build:watch": "esbuild src/code.tsx --bundle --outfile=dist/code.js --format=esm --target=es6 --jsx=transform --jsx-factory=figma.widget.h --jsx-fragment=figma.widget.Fragment --watch",
    "dev": "npm run build:watch",
    "dev:full": "concurrently -n tsc,build 'npm run test -- --preserveWatchOutput --watch' 'npm run build:watch'"
  },
  "author": "",
  "license": "MIT",
  "keywords": [
    "figma",
    "widget",
    "design",
    "signoff"
  ],
  "dependencies": {
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.49.0",
    "@figma/widget-typings": "^1.4.0",
    "concurrently": "^6.4.0",
    "esbuild": "^0.13.15"
  }
}
